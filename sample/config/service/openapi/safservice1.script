var result = server.doService("server3",bizmessage.data)
if (result.code == 0) {
    if (result.data.step == null) {
        result.data.step = 1;
    }
    else {
        result.data.step = result.data.step + 1;
    }
    log.info("sip_done_num:{}",server.getTmRetryCount());
    server.setTmDelayTime(server.getTmRetryCount() * 2000);
    if (server.getTmRetryCount() > 5) {
        if (result.data.sex == "1") {
            server.setTmServiceStatus("success");
        }
        else {
            server.setTmServiceStatus("error");
        }
    }
    return result.data;
}
else {
    var error_result = {
        code: result.code,
        message: result.message
    }
    return error_result;
}